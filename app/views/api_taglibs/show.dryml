<show-page title="#{this.name.titleize}">

  <aside-content:>
    <api-navigation current="&this" with="&this.library"/>
  </aside-content:>
  
  <content:>
    <div class="content-header">
      <h2><%= this.name.titleize %></h2>      
    </div>
    <div class="content-body">
      <view:short_description/>
      <view:description/>
      <tags-summary/>
    </div>
  </content:>
  
</show-page>